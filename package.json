        "gulp-tslint": "^6.1.2",
        "gulp-typescript": "~3.0.2",

        "tslint": "^3.15.1",
        "typescript": "2.0.6",
        "typings": "~1.4.0"
    },

    "dependencies": {
        "body-parser": "~1.15.1",
        "cookie-parser": "~1.4.3",
        "consign": "~0.1.2",
        "debug": "~2.2.0",
        "express": "~4.14.0",
        "morgan": "~1.7.0"
    }

